<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net9.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <Authors>Filip Jeremic, Joshua Foster</Authors>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Description>A WPF control for displaying binary data in a traditional hex view.</Description>
    <PackageProjectUrl>https://github.com/josh2112/HexView.Wpf</PackageProjectUrl>
    <RepositoryUrl>https://github.com/josh2112/HexView.Wpf</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>WPF;HexView;HexViewer</PackageTags>
    <Version>0.1.0</Version>
    <PackageReleaseNotes></PackageReleaseNotes>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <Folder Include="Converters\" />
  </ItemGroup>
  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath>
      </PackagePath>
    </None>
  </ItemGroup>
</Project>